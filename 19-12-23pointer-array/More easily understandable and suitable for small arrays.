#include <stdio.h>
#include<string.h>
#include<stdlib.h>
void printpArr(char** str, int len);
void runpArr(char** str, int len);

int main(int argc,char* argv[]){
	char* pArr[] = { "asd","csf","as","qwf"};
	int len = sizeof(pArr) / sizeof(char*);
	runpArr(pArr, len);
	printpArr(pArr, len);
	return 0;
}

void runpArr(char** str, int len){
	char* temp;
	for (int i = 0; i < len; i++){
		for (int j = i + 1; j < len; j++)
			if (strcmp(str[i], str[j]) > 0) {
				temp = str[i];
				str[i] = str[j];
				str[j] = temp;
			}
	}
}

void printpArr(char** str, int len){
	for (int i = 0; i < len; i++)
		printf("%s ", str[i]);
}
